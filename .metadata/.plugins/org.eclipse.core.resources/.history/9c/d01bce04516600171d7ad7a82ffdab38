package fileReadOrWrite.demo;

import java.io.File;

import org.apache.commons.io.FileUtils;

/**
 *  @author Jion
 *  语言的处理类
 */
public class ReplaceChineseUntil {

	/**删除txt文档中的汉语*/
	public void ReplaceChinese() {
		File file = FileUtils.getFile("/JavaBase/src/fileReadOrWrite/demo/姓名.txt");
		FileUtils.readLines(file, "")
	}
}
