package com.model.proxy;

import java.util.Random;

/**汽车类*/
public class Car implements Movable {

	@Override
	public void move() {
		try {
			Thread.sleep(new Random().nextInt(1000));
			System.out.println("汽车行驶中......");
		} catch (InterruptedException e) {
			e.printStackTrace();
			System.err.println("睡眠中出现异常");
		}
	}

}
