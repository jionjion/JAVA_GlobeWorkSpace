package java.model.singleton;
/**单例模式,饿汉方法*/
public class SingletonByHungryMan {

	//1.私有构造方法,拒绝外部直接创建对象
	private SingletonByHungryMan(){}
	
	//2.创建实例,唯一
	private SingletonByHungryMan instance = new SingletonByHungryMan();
	
	//3.提供一个静态的外部方法
	public static SingletonByHungryMan getInstance() {
		return instance;
	}
}
