package annotationBean;

import java.io.Serializable;

import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

/**
 *	学生类,为多方.
 */
public class APupil implements Serializable{

	private static final long serialVersionUID = 1L;
	
	@Id
	@GeneratedValue(strategy=GenerationType.AUTO)
	private int pid;										//学生类主键
	
	private String pname;
	
	private String psex;
	
	private int gid;										//多方持有一方的引用
	
	//多对一配置,班级的引用.
	private AGrade grade;
	
	public APupil(){}
	
	public APupil(int pid, String pname, String psex, int gid) {
		super();
		this.pid = pid;
		this.pname = pname;
		this.psex = psex;
		this.gid = gid;
	}


	public int getPid() {
		return pid;
	}
	public void setPid(int pid) {
		this.pid = pid;
	}
	public String getPname() {
		return pname;
	}
	public void setPname(String pname) {
		this.pname = pname;
	}
	public String getPsex() {
		return psex;
	}
	public void setPsex(String psex) {
		this.psex = psex;
	}
	public int getGid() {
		return gid;
	}
	public void setGid(int gid) {
		this.gid = gid;
	}

	
	public AGrade getGrade() {
		return grade;
	}

	public void setGrade(AGrade grade) {
		this.grade = grade;
	}

	@Override
	public String toString() {
		return "Pupil [pid=" + pid + ", pname=" + pname + ", psex=" + psex + ", gid=" + gid + "]";
	}
	
	
}
