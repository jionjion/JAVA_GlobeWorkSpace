package networkCommunication.TCPNetwork;

import java.net.Socket;

/**	服务器端,进行处理类*/
public class ServerThread extends Thread {

	//和本线程相关的socket
	Socket socket = new Socket();
	
	public ServerThread(Socket socket) {
		this.socket = socket;
	}
	
	/**重写线程方法,调用一个线程,相应客户端的请求*/
	@Override
	public void run() {
		
	}
}
