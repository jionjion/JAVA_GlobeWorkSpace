package com.model.singleton;
/**	单例模式,饱汉方法*/
public class SingletonByPlumpMan {

	//1.私有构造方法,拒绝外部直接创建对象
	private SingletonByPlumpMan(){}
	
	//2.创建类实例,唯一			private static
	private static SingletonByPlumpMan instance ;
	
	//3.提供一个静态类的外部方法	public static
	public static SingletonByPlumpMan getInstance() {
		if (instance == null) {
			SingletonByPlumpMan instance = new SingletonByPlumpMan();
		}
		return instance;
	}
}
