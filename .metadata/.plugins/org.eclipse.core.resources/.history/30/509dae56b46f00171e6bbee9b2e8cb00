package annotationBean;

import javax.persistence.CascadeType;
import javax.persistence.Entity;
import javax.persistence.JoinColumn;
import javax.persistence.OneToOne;


/**	学生卡对象
 * 	作为一对一的协助方*/
@Entity
public class AStudentCard {

	private int cid;
	
	private String description;

	@OneToOne(cascade=CascadeType.ALL)			//设置级联关系
	@GeneratedValue(generator="sid")						//主键生成策略,采用主键生成器
	@GenericGenerator(name="sid",strategy="assigned")		//自定义主键生成器,主键为手工指定
	private APupil aPupil;
	
	public int getCid() {
		return cid;
	}

	public void setCid(int cid) {
		this.cid = cid;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public APupil getaPupil() {
		return aPupil;
	}

	public void setaPupil(APupil aPupil) {
		this.aPupil = aPupil;
	}

	public AStudentCard() {
		super();
	}	
	
	
}
