package MicroServices;


import javax.persistence.Entity;
import javax.persistence.GeneratedValue;

import org.springframework.data.annotation.Id;

/**	数据库的映射类*/
@Entity
public class User {

	@Id
	@GeneratedValue
	private Integer id;
	
	private String username;
	
	private String password;

	public User() {
		super();
	}
	
}
