
	
		<#switch o>								<!-- 对变量进行选择 -->
			<#case "字符">							
			<#case "字符串">							<!-- 多个情况下,执行 -->
				<p>对象为字符或字符串</p>
			<#break>								<!-- 停止继续向下执行 -->
			<#case 100>								<!-- 判断的数据类型可以为数字,字符等 -->
				<p>对象为数字100</p>		
			<#break>
			<#default>								<!-- 如果都不匹配,则走默认的情况 -->
				<p>对象类型未知</p>
		</#switch>